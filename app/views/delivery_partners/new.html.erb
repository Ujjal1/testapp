<div class="container">
  <h2>Onboard Delivery Partner</h2>
  <%= form_with model: @delivery_partner, url: delivery_partners_path, method: :post, class: "form" do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :email %>
      <%= f.text_field :email, class: "form-control" %>
    </div>
    <%= f.submit "Create Delivery Partner", class: "btn btn-primary" %>
  <% end %>
</div>